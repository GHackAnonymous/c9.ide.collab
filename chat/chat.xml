<a:application xmlns:a="http://ajax.org/2005/aml">
    <a:menu id="mnuCtxTree" />
    <a:vsplitbox id="winCollab" splitter="true">
      <a:vbox height="50%" width="100%">
        <a:label class="collabPanelHeading">Workspace Members</a:label>
        <a:bar id="membersTree" border="0" margin="0 0 0 0" flex="1"
                contextmenu="mnuCtxTree"></a:bar>
      </a:vbox>

      <a:vbox>
        <a:label class="collabPanelHeading">Group Chat</a:label>
        <a:bar skin="panel-bar" flex="1">
            <div id="chatText" class="authorColors"></div>
            <a:codebox id="chatInput" realtime="true" skin="codebox" clearbutton="true"
              focusselect="true" left="5" bottom="5" right="5" />
        </a:bar>
      </a:vbox>
    </a:vsplitbox>
</a:application>
